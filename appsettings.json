{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedOrigin": "https://localhost:7090",
  "FeUrl": "https://localhost:7090",
  "ApiBaseUrl": "https://localhost:7224",
  "Stripe": {
    "SecretKey": "${Stripe_SecretKey}",
    "WebhookSecret": "${Stripe_WebhookSecret}"
  },
  "GitHub": {
    "ClientId": "${GitHub_ClientId}",
    "ClientSecret": "${GitHub_ClientSecret}"
  },
  "Jwt": {
    "Secret": "${Jwt_Secret}",
    "Issuer": "myIssuer",
    "Audience": "myAudience"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DefaultConnection}",
    "AzureBlobStorage": "${AzureBlobStorage}"
  },
  "ApplicationInsights":
  {
     "InstrumentationKey": "${InstrumentationKey}"
  }
}
